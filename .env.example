# Spring Boot Configuration
SPRING_PROFILES_ACTIVE=deploy

# MySQL Configuration
MYSQL_ROOT_PASSWORD=rootPass123!
MYSQL_DATABASE=appdb
MYSQL_USER=appuser
MYSQL_PASSWORD=appPass123!

# Ollama Configuration
